{
  "name": "ChatApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:install": "bundle install && bundle exec pod install --project-directory=ios",
    "ios:clean": "cd ios && rm -rf Pods Podfile.lock && cd .. && yarn ios:install",
    "start": "react-native start",
    "test": "jest",
    "test:affected": "jest --changedSince=origin/main --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"App.tsx\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"App.tsx\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" \"App.tsx\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" \"App.tsx\"",
    "typecheck": "tsc --noEmit",
    "validate": "yarn format:check && yarn lint && yarn typecheck",
    "prepare": "husky",
    "version:patch": "npm version patch --no-git-tag-version && yarn version:sync",
    "version:minor": "npm version minor --no-git-tag-version && yarn version:sync",
    "version:major": "npm version major --no-git-tag-version && yarn version:sync",
    "version:sync": "node scripts/sync-version.js"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.83.0",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "@tanstack/react-query": "^5.90.12",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-config": "^1.6.1",
    "react-native-device-info": "^15.0.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@jest/test-sequencer": "^30.2.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@testing-library/react-native": "^13.3.3",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
